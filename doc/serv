openServer()
gameLoop()
  initGame()
  spawnThreads()
    waitJoin()
    sendPrompt()
    waitReady()
    roundLoop()
      initRound()
      sendStatus()
      waitReady()
      sendQuestion()
      waitAnswer()
      processRound()
      isDoneGame()
      sendRoundLoopControl()
    processResult()
    sendResult()
    sendPrompt()
    waitAllResponse()
    isDone()
    sendGameLoopControl()
closeServer()
 
